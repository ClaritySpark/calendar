    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "typescript": "5.3.3",
    "@vercel/style-guide": "5.2.0",
    "esbuild": "0.20.2"
  }
